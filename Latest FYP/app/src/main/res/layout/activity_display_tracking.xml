<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.choonket.fyp.displayTracking">

    <Spinner
        android:id="@+id/spinnerCategoryT"
        android:layout_width="174dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:textSize="15sp"
        android:layout_marginTop="17dp" />

    <include
        android:id="@+id/test1"
        layout="@layout/content_main2"
        android:layout_width="381dp"
        android:layout_height="481dp"
        android:layout_centerInParent="true"
        android:textSize="15sp" />

    <Spinner
        android:id="@+id/spinnerDistance"
        android:layout_width="119dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/spinnerCategoryT"
        android:layout_marginEnd="97dp" />

    <Button
        android:id="@+id/btnTracking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="21dp"
        android:onClick="buttonOnClick"
        android:text="Go" />

</RelativeLayout>
